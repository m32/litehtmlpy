									<xsd:element name="P_101" type="etd:TZnakowy">
										<xsd:annotation>
											<xsd:documentation>Rodzaj przychodu (wymienić)</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="P_102" type="etd:TKwota2Nieujemna">
										<xsd:annotation>
											<xsd:documentation>Przychód</xsd:documentation>
										</xsd:annotation>
