								<xsd:sequence>
									<xsd:element name="P_118" type="etd:TWybor1" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Rodzaj (podstawa prawna) przychodu zwolnionego od podatku, wykazanego w poz. 114: 1 - art. 21 ust. 1 pkt 152 ustawy</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="P_119" type="etd:TWybor1" minOccurs="0">
